<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="jansen.thomas.mrpotatohead.MainActivity.MainActivity">

    <ImageView
        android:id="@+id/imageBody"
        android:layout_width="339dp"
        android:layout_height="342dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:contentDescription="@body"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.13999999"
        app:srcCompat="@drawable/body" />

    <ImageView
        android:id="@+id/imageArms"
        android:layout_width="341dp"
        android:layout_height="342dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:contentDescription="@arms"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.13999999"
        app:srcCompat="@drawable/arms" />

    <ImageView
        android:id="@+id/imageEars"
        android:layout_width="339dp"
        android:layout_height="344dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:contentDescription="@ears"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.13999999"
        app:srcCompat="@drawable/ears" />

    <ImageView
        android:id="@+id/imageEyebrows"
        android:layout_width="340dp"
        android:layout_height="344dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:contentDescription="@eyebrows"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.13999999"
        app:srcCompat="@drawable/eyebrows" />

    <ImageView
        android:id="@+id/imageEyes"
        android:layout_width="339dp"
        android:layout_height="342dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:contentDescription="@eyes"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.13"
        app:srcCompat="@drawable/eyes" />

    <ImageView
        android:id="@+id/imageGlasses"
        android:layout_width="339dp"
        android:layout_height="345dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:contentDescription="@glasses"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.13"
        app:srcCompat="@drawable/glasses" />

    <ImageView
        android:id="@+id/imageHat"
        android:layout_width="339dp"
        android:layout_height="344dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:contentDescription="@hat"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.13"
        app:srcCompat="@drawable/hat" />

    <ImageView
        android:id="@+id/imageMouth"
        android:layout_width="342dp"
        android:layout_height="345dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:contentDescription="@mouth"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.13"
        app:srcCompat="@drawable/mouth" />

    <ImageView
        android:id="@+id/imageMoustache"
        android:layout_width="340dp"
        android:layout_height="341dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:contentDescription="@moustache"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.13"
        app:srcCompat="@drawable/mustache" />

    <ImageView
        android:id="@+id/imageNose"
        android:layout_width="342dp"
        android:layout_height="343dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:contentDescription="@nose"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.13"
        app:srcCompat="@drawable/nose" />

    <ImageView
        android:id="@+id/imageShoes"
        android:layout_width="340dp"
        android:layout_height="342dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:contentDescription="@shoes"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.143"
        app:srcCompat="@drawable/shoes" />

    <CheckBox
        android:id="@+id/checkBoxHat"
        android:layout_width="94dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="28dp"
        android:layout_marginStart="64dp"
        android:checked="false"
        android:duplicateParentState="false"
        android:text="Hat"
        app:layout_constraintBottom_toTopOf="@+id/checkBoxEyebrows"
        app:layout_constraintEnd_toStartOf="@+id/checkBoxArms"
        app:layout_constraintStart_toStartOf="parent"
        android:onClick="checkClicked"/>

    <CheckBox
        android:id="@+id/checkBoxArms"
        android:layout_width="94dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="78dp"
        android:text="Arms"
        app:layout_constraintBottom_toTopOf="@+id/checkBoxMoustache"
        app:layout_constraintEnd_toEndOf="parent"
        android:onClick="checkClicked"/>

    <CheckBox
        android:id="@+id/checkBoxEyebrows"
        android:layout_width="94dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="28dp"
        android:layout_marginStart="64dp"
        android:text="Eyebrows"
        app:layout_constraintBottom_toTopOf="@+id/checkBoxEyes"
        app:layout_constraintEnd_toStartOf="@+id/checkBoxMoustache"
        app:layout_constraintStart_toStartOf="parent"
        android:onClick="checkClicked"/>

    <CheckBox
        android:id="@+id/checkBoxEyes"
        android:layout_width="94dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="28dp"
        android:layout_marginStart="64dp"
        android:text="Eyes"
        app:layout_constraintBottom_toTopOf="@+id/checkBoxGlasses"
        app:layout_constraintEnd_toStartOf="@+id/checkBoxMouth"
        app:layout_constraintStart_toStartOf="parent"
        android:onClick="checkClicked"/>

    <CheckBox
        android:id="@+id/checkBoxGlasses"
        android:layout_width="94dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="28dp"
        android:layout_marginStart="64dp"
        android:text="Glasses"
        app:layout_constraintBottom_toTopOf="@+id/checkBoxNose"
        app:layout_constraintEnd_toStartOf="@+id/checkBoxEars"
        app:layout_constraintStart_toStartOf="parent"
        android:onClick="checkClicked"/>

    <CheckBox
        android:id="@+id/checkBoxNose"
        android:layout_width="94dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="28dp"
        android:layout_marginStart="64dp"
        android:text="Nose"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/checkBoxShoes"
        app:layout_constraintStart_toStartOf="parent"
        android:onClick="checkClicked"/>

    <CheckBox
        android:id="@+id/checkBoxMoustache"
        android:layout_width="104dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="68dp"
        android:text="Moustache"
        app:layout_constraintBottom_toTopOf="@+id/checkBoxMouth"
        app:layout_constraintEnd_toEndOf="parent"
        android:onClick="checkClicked"/>

    <CheckBox
        android:id="@+id/checkBoxMouth"
        android:layout_width="94dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="78dp"
        android:text="Mouth"
        app:layout_constraintBottom_toTopOf="@+id/checkBoxEars"
        app:layout_constraintEnd_toEndOf="parent"
        android:onClick="checkClicked"/>

    <CheckBox
        android:id="@+id/checkBoxEars"
        android:layout_width="94dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="78dp"
        android:text="Ears"
        app:layout_constraintBottom_toTopOf="@+id/checkBoxShoes"
        app:layout_constraintEnd_toEndOf="parent"
        android:onClick="checkClicked"/>

    <CheckBox
        android:id="@+id/checkBoxShoes"
        android:layout_width="94dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="78dp"
        android:text="Shoes"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:onClick="checkClicked"/>

</android.support.constraint.ConstraintLayout>
